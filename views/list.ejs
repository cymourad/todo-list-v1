<%- include("header") -%>
  <body>

    <div class="box" id="heading">
      <h1><%=listTitle%> List</h1>
    </div>

    <div class="box">

      <ul>
        <% for (var i = 0; i<htmlItems.length; i++) { %>
          <div class="item">
            <input type="checkbox" >
              <p><%=htmlItems[i]%></p>
          </div>
        <%  } %>
      </ul>

      <form class="item" action="/" method="post">
        <input type="text" name="toDoItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="submitItem" value=<%=listTitle%>>+</button>
      </form>

    </div>


  </body>

<%- include("footer") -%>
